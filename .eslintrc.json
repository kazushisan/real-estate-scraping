{
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"		
	},
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"airbnb-base",
		"plugin:prettier/recommended"
	],
	"rules": {
		"no-await-in-loop": 0,
		"no-plusplus": 0,
		"import/extensions": [2, "always"]
	}
}
